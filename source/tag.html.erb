---
pageable: false
per_page: 12
layout: article
---
<h2>Posts about <%= tagname %>.</h2>

<% if paginate %>
  <p>Page <%= page_number %> of <%= num_pages %></p>

  <% if prev_page %>
    <p><%= link_to 'Previous page', prev_page %></p>
  <% end %>
<% end %>

<ul class="post_list">
  <% page_articles.each_with_index do |article, i| %>
    <li><%= link_to article.title, article %> <span><%= date_tag(article.date) %></span></li>
  <% end %>
</ul>

<% if paginate %>
  <% if next_page %>
    <p><%= link_to 'Next page', next_page %></p>
  <% end %>
<% end %>
